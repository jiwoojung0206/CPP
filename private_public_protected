
#include <iostream>
#include <vector>
#include <string>

using namespace std;

class Player {
    private:
    //attributes
    string name;
    int health;
    int xp;
    
    public:
    //methods
    void talk(string text) {cout << name << ": " << text;}
    bool is_dead();
};

int main() {
    
    Player Jiwoo;
    Jiwoo.name = "Jiwoo";
    
    cout << Jiwoo.health << endl;
    
    
    cout << endl;
    return 0;
}
